<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <%- include("./shared/head") %>

</head>
<body>
    <h1>Hello from signup</h1>
    <form class="form" action="/signup" method="post">
        <label for="Name">User Name :</label>
        <input type="text" name="Name" required>
        <label for="Email"> Email :</label>
        <input type="email" name="Email" required>
        <label for="Linkedin"> Linkedin :</label>
        <input type="text" name="Linkedin" required>
        <label for="Facebook"> Facebook :</label>
        <input type="text" name="Facebook" required>
        <label for="Instagram"> Instagram :</label>
        <input type="text" name="Instagram" required>
        <label for="Password"> Password :</label>
        <input type="password" name="Password" required>
        <label for="Bio"> Tell us more about your self :</label>
        <input type="text" name="Bio"  placeholder="Bio" required >
        
        <button>Sign up</button>
    </form>
</body>
</html>